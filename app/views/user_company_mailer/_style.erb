.ReadMsgBody {
	width: 100%;
	background-color: #ffffff;
}
.ExternalClass {
	width: 100%;
	background-color: #ffffff;
}
body {
	width: 100%;
	background-color: #ffffff;
	margin: 0;
	padding: 0;
	-webkit-font-smoothing: antialiased;
	font-family: "Helvetica Neue", "Helvetica", Helvetica, Arial, sans-serif;
}
table {
	border-collapse: collapse;
}
.claim {
	font-size: 13px;
	color: #272727;
	font-weight: light;
	text-align: right;
	font-family: Georgia, Times, serif;
	line-height: 20px;
	vertical-align: baseline;
	padding: 10px 20px;
	font-style: italic;
}

.header {
	padding-top: 20px;
}

.logo {
	padding: 10px 20px;
}

.welcome {
	padding: 0;
}

.welcome img {
	display: block;
	border-radius: 4px;
}

.section {
	font-size: 13px;
	color: #959595;
	font-weight: normal;
	text-align: left;
	font-family: Georgia, Times, serif;
	line-height: 24px;
	vertical-align: top;
	padding: 0px 20px 20px 20px;
}

.section table td {
	padding: 0 0 0 5px;
}

.title {
	text-decoration: none;
	color: #272727;
	font-size: 16px;
	color: #272727;
	font-weight: bold;
	font-family: Arial, sans-serif;
}

.title.inverse {
	color: #ccc;
}

.subtitle {
	text-decoration: none;
	font-size: 14px;
	color: #eee;
	font-weight: bold;
	font-family: Arial, sans-serif;
}

p.small {
	color: #ddd;
	text-align: left;
	font-size: 10px;
	line-height: 17px;
}

p.msotable {
	mso-table-lspace: 0;
	mso-table-rspace: 0;
	margin: 0;
}

div.spacer {
	height: 15px;
}

td.spacer {
	padding: 10px 0;
}

td.hspacer {
	padding: 0 10px;
}


td.dspacer {
	padding: 10px;
}

b {
	color: #5b5b5b;
}

img.big {
	border-radius: 4px;
	width: 267px;
	display: block;
}

img.small {
	border-radius: 4px;
	width: 170px;
	display: block;
}

.button {
	padding: 5px 0;
	background-color: #8185b1;
	border-top: 1px solid #c7c9d8;
}

.button.grey {
	padding: 5px 0;
	background-color: #c8c8ca;
	border-top: 1px solid #eee;
}

.button.grey a {
	color: #333;
}

.button a {
	color: #ffffff;
	font-size: 13px;
	font-weight: bold;
	text-align: center;
	text-decoration: none;
	font-family: Arial, sans-serif;
	-webkit-text-size-adjust: none;
}

.thickline {
	height: 6px;
}

.underline {
	border-bottom: 1px solid #333;
}

.date {
	color: #fff;
	font-size: 12px;
	font-weight: bold;
	text-align: center;
	-webkit-text-size-adjust: none;
	padding: 4px 8px;
}

.headline {
	font-size: 11px;
	color: #656565;
	text-align: left;
	padding-left: 8px;
}

.headline a {
	font-size: 14px;
	font-weight: bold;
	color: #000000;
	text-align: left;
	text-decoration: none;
}

.legal {
	font-size: 11px;
	color: #f1f1f1;
	color: #999;
	font-family: Arial, sans-serif;
	padding-bottom: 20px;
}

.legal a {
	color: #999;
	text-decoration: underline;
}
