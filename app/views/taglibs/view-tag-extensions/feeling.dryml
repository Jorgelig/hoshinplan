<def tag='view' for="EnumFeeling">
	<span class="ic-#{this} feeling-ic"></span>
</def>

<def tag="input" for="EnumFeeling">
	<radio-group options="&EnumFeeling.values" id="#{this_parent.id}"/>
</def>

<def tag="radio-button-item" attrs="option, id" for="EnumFeeling">
	<radio-button option="&option" id="#{id}-#{option}" param /> 
	<label for="#{id}-#{option}"><span class="ic-#{option}"></span><span class="feeling-option-label">#{option}</span></label>
</def>